    "import math\n",
    "from tabulate import tabulate\n",
    "i=0\n",
    "fun=0\n",
    "tab=[]\n",
    "while i<10:\n",
    "    fun=math.exp(-fun)\n",
    "    Erpv=math.fabs(((0.5671-fun)/0.5671)*100)\n",
    "    tab.append([i+1,fun,Erpv])\n",
    "    i+=1\n",
    "print tabulate (tab,headers=[\"Iteracion\", \"Raiz\", \"Et\"],tablefmt='orgtbl')"
   ]
  }
